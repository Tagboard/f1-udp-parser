{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "rootDir": ".",
    "outDir": "build",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "target": "ESNext",
    "moduleResolution": "node"
  },
  "include": ["src/*.ts", "src/**/*.ts", "test/*.ts", "test/**/*.ts"]
}
